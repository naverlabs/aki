+        cprint_lcd_update(FONT_RED, FONT_BLACK, 40, "RAMDUMP MODE   ");
+		break;
+	default:
+		break;
+	}
+
+	if (mode != PANIC_NONE) {
+		fastboot_splash = 0;
+		run_command("fastboot", 0);
+	}
+
+	return 0;
+}
