+void vib_turn_on(int on)
+{
+	struct s5p_gpio_bank *bank = 0x139f0040;
+	static int vib_init = 0;
+	if(vib_init == 0){
+		s5p_gpio_set_pull(bank, 7, GPIO_PULL_DOWN);
+		s5p_gpio_cfg_pin(bank, 7, GPIO_OUTPUT);
+		s5p_gpio_set_drv(bank, 7, GPIO_DRV_4X);
+		vib_init = 1;
+	}
+	if(on){
+		s5p_gpio_set_value(bank, 7, 1);
+		speedy_write(S2MPW01_PM_ADDR, S2MPW01_PM_LDO18_CTRL, 0xda);
+	} else {
+		s5p_gpio_set_value(bank, 7, 0);
+		speedy_write(S2MPW01_PM_ADDR, S2MPW01_PM_LDO18_CTRL, 0x00);
+	}
