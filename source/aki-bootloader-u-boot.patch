+static int check_repartition(void)
+{
+	unsigned int start, count;
+	unsigned char pid;
+	int *p = 0x50000000;
+	char cmd[128];
+
+	/* get persistent partition info */
+	get_boot_part_info(0, 11, &start, &count, &pid);
+
+	/* read first sector and check sparse image exist */
+	sprintf(cmd, "mmc read 0 0x%x 0x%x 0x1", (int)p, start);
+	printf("part: %s\n", cmd);
+	run_command(cmd, 0);
+
+	if (*p == 0xed26ff3b) {
+		printf("part: found sparse image signature!\n");
+		cprint_lcd_update(FONT_DARK_GRAY, FONT_BLACK, 40, "repartition    ");
+
+		/* write gpt info */
+		cprint_lcd_update(FONT_DARK_GRAY, FONT_BLACK, 40, "repartition.   ");
+		printf("part: write gpt info\n");
+		run_command("gpt write mmc 0", 0);
+
+		/* read sparse image in persistent partition to buffer */
+		cprint_lcd_update(FONT_DARK_GRAY, FONT_BLACK, 40, "repartition..  ");
+		sprintf(cmd, "mmc read 0 0x%x 0x%x 0x%x", (int)p, start, count);
+		printf("part: %s\n", cmd);
+		run_command(cmd, 0);
+
+		/* flash sparse image to cache partition using fastboot flash command */
+		cprint_lcd_update(FONT_DARK_GRAY, FONT_BLACK, 40, "repartition... ");
+		fastboot_splash = 0;
+		sprintf(cmd, "fastboot flash cache %x", (int)p);
+		printf("part: %s\n", cmd);
+		run_command(cmd, 0);
+
+		/* clear sparse image signature on persistent partition */
+		cprint_lcd_update(FONT_DARK_GRAY, FONT_BLACK, 40, "repartition....");
+		*p = 0;
+		sprintf(cmd, "mmc write 0 0x%x 0x%x 0x1", (int)p, start);
+		printf("part: %s\n", cmd);
+		run_command(cmd, 0);
+
+		/* done */
+		cprint_lcd_update(FONT_BLACK, FONT_BLACK, 40, "repartition....");
+
+		return 1;
+	}
+
+	return 0;
+}
+
+static int check_persistent(void)
+{
+	unsigned int start, count;
+	unsigned char pid;
+	int *p = 0x50000000;
+	int r = 1;
+	char cmd[128];
+
+	/* get persistent partition info */
+	get_boot_part_info(0, 11, &start, &count, &pid);
+
+	/* read first sector and check command exist */
+	sprintf(cmd, "mmc read 0 0x%x 0x%x 0x1", (int)p, start);
+	printf("persist: %s\n", cmd);
+	run_command(cmd, 0);
+
+	/* handle command */
+	switch (*p) {
+		case 0x3aec3353:
+			setenv("debug_ctl", "enabled");
+			break;
+		case 0xcb07acb5:
+			setenv("debug_ctl", "disabled");
+			break;
+		default:
+			r = 0;
+			break;
+	}
+
+	if (r) {
+		/* save env first */
+		saveenv();
+
+		/* clear command in persistent partition */
+		*p = 0;
+		sprintf(cmd, "mmc write 0 0x%x 0x%x 0x1", (int)p, start);
+		printf("persist: %s\n", cmd);
+		run_command(cmd, 0);
+	}
+
+	return r;
+}
