+#ifdef CFG_LOWPOWER_SHUTDOWN
+/* NAVERLABS: LOW POWER SHUTDOWN */
+int lowbatt_shutdown = 0;
+extern u32 s2mpw01_get_vbat(void);
+
+int is_low_batt(void)
+{
+	int r;
+	unsigned int vbat;
+	unsigned char pwronsrc;
+	char *p;
+
+	speedy_read(S2MPW01_PM_ADDR, S2MPW01_PM_PWRONSRC, &pwronsrc);
+
+	if (pwronsrc != 0x1) {
+		printf("is_low_batt: skip low battery check for if not powered on by power key.\n");
+		r = 0;
+	} else {
+		vbat = s2mpw01_get_vbat();
+
+		p = getenv("lowbatt");
+		if (p && !strcmp(p, "1")) {
+			printf("is_low_batt: env lowbatt is 1. set lowbatt\n");
+			r = 1;
+		} else if (p && !strcmp(p, "2")) {
+			printf("is_low_batt: env lowbatt is 2. set normal\n");
+			r = 0;
+		} else {
+			r = (vbat < 3550) ? 1 : 0;
+		}
+
+		printf("is_low_batt: vbat=%d, return %s\n", vbat, (r) ? "lowbatt" : "normal");
+	}
+
+	return r;
+}
+#endif
