+/* NAVERLABS
+ * static int check_power_key_function()
+ *  - stage 1 : FWDOWN MODE
+ *  - stage 2 : ENGBOOT MODE
+ *  - stage 3 : RAMDUMP MODE
+ */
+#define POWER_KEY_MAX_STAGE	3
+
+static int check_power_key_function(int stage)
+{
+	unsigned char status1_value = 0;
+	unsigned long time_start, time_prev, time_now;
+
+	time_start = get_timer(0);
+	time_prev = time_start;
+	time_now = time_start;
+
+	switch (stage) {
+		case 1:
+			cprint_lcd_update(FONT_DARK_GRAY, FONT_BLACK, 40, "FWDOWN MODE    ");
+			break;
+		case 2:
+			cprint_lcd_update(FONT_GREEN, FONT_BLACK, 40, "ENGBOOT MODE   ");
+			break;
+		case 3:
+			cprint_lcd_update(FONT_RED, FONT_BLACK, 40, "FORCED DUMP MODE ");
+			break;
+		default:
+			break;
+	}
+
+	/* if we reach last stage, then just fall into the stage */
+	if (stage == ((disable_debug_key) ? 1 : POWER_KEY_MAX_STAGE))
+		return stage;
+
+    while ((time_now - time_start) <= (3 * DELAY_1S)) {
+        time_now = get_timer(0);
+        if ((time_now - time_prev) >= DELAY_500MS) {
+            speedy_read(S2MPW01_PM_ADDR, S2MPW01_PM_STATUS1, &status1_value);
+            if (!(status1_value & 0x01)) {
+				/* fall into current stage */
+                return stage;
+            }
+            time_prev = time_now;
+        }
+    }
+
+	/* let's go to the next stage */
+	return check_power_key_function(stage + 1);
+}
+
+/*
+	S2MPW01_for_JavaW_Datasheet_REV0.40_Preliminary_160527.pdf
+	P75		5.2.2.1 INT1
+		PWRON1S [5] R 1 : PWRON high for longer than 1 sec N/A
+		PWRONR [1] RC 1 : PWRON rising event interrupt
+		PWRONF [0] RC 1 : PWRON falling event interrup
+	P78		5.2.2.7 STATUS1
+		PWRON [0] R b0 : PWRON is low (No input) 1 : PWRON is high N/A
+
+	P79		5.2.2.9 PWRONSRC
+		PWRON [0] R 1 : PMIC On by PWRON key
+*/
+
+static int is_power_key_pressed(int duration, int count)
+{
+	unsigned char int1_value = 0;
+	unsigned long time_start, time_prev, time_now;
+	int key_count = 0;
+
+	time_start = get_timer(0);
+	time_prev = time_start;
+	time_now = time_start;
+
+	while ((time_now - time_start) <= (duration * DELAY_1S)) {
+		time_now = get_timer(0);
+
+		/* check interrupt every 100ms */
+		if ((time_now - time_prev) >= DELAY_100MS) {
+			speedy_read(S2MPW01_PM_ADDR, S2MPW01_PM_INT1, &int1_value);
+			if (int1_value & 0x02) {
+				if (++key_count > 0) {
+					printf("power key pressed!!\n");
+				}
+				if (key_count == count) {
+					return 1;
+				}
+			}
+			time_prev = time_now;
+		}
+	}
+
+	return 0;
+}
