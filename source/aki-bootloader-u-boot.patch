+bool getchargingmode(void)
+{
+    bool status=false;
+    unsigned char chgin_input_stat = 0;
+    unsigned char read_pwronsrc = 0;
+    enum reboot_mode_t reboot_mode = REBOOT_NORMAL;
+    speedy_read(S2MPW01_CHG_ADDR, S2MPW01_CHG_STATUS1, &chgin_input_stat);
+    speedy_read(S2MPW01_PM_ADDR, S2MPW01_PM_PWRONSRC, &read_pwronsrc);
+    reboot_mode = dfd_display_reboot_reason(read_pwronsrc);
+
+#if defined(CONFIG_POWEROFF_CHARGER)
+    if ((reboot_mode == REBOOT_NORMAL) && (read_pwronsrc == 0x4) && ((chgin_input_stat & 0x10) == 0x10)) {
+        //boot_mode = BOOTMODE_CHARGER;
+        printf("bootmode is charger\n");
+        status = true;
+    }
+#endif
+
+    return status;
+}
