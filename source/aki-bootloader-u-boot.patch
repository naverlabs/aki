+	0xD3, 0x06, 0xA1, 0x06, 0x7A, 0x06, 0x5C, 0x06, 0x43, 0x06,
+	0x29, 0x06, 0x05, 0x06, 0xDD, 0x05, 0xA1, 0x05, 0x84, 0x05,
+	0xEF, 0x04, 0xD3, 0x02, 0x66, 0x08, 0x00, 0x08, 0x98, 0x07,
+	0x2F, 0x07, 0xC7, 0x06, 0x5F, 0x06, 0xF6, 0x05, 0x8E, 0x05,
+	0x25, 0x05, 0xBD, 0x04, 0x55, 0x04, 0xEC, 0x03, 0x84, 0x03,
+	0x1B, 0x03, 0xB3, 0x02, 0x4B, 0x02, 0xE2, 0x01, 0x7A, 0x01,
+	0x12, 0x1, 0xA9, 0x0, 0x40, 0x0, 0xF4, 0x0F
+};
+
+static unsigned char model_param3[] = {
+	0xBC, 0xBC, 0xC1, 0xC7, 0xC8, 0xCC, 0xCF, 0xD1, 0xCE, 0xCE,
+	0xCF, 0xD0, 0xD4, 0xD5, 0xD4, 0xD5, 0xD5, 0xD7, 0xD5, 0xD3,
+	0xDD, 0xFF 
+};
+
+static u16 s2mpw01_read_fg_register_word(u8 addr)
+{
+	u8 msb = 0;
+	u8 lsb = 0;
+
+	PMIC_FUELGAUGE_READ(addr++, &lsb);
+	PMIC_FUELGAUGE_READ(addr, &msb);
+	return ((msb << 8) | lsb);
+}
+
+u32 s2mpw01_get_vbat(void)
+{
+	u16 data;
+	u32 vbat;
+
+	/* Return vbat value in mV */
+	data = s2mpw01_read_fg_register_word(S2MPW01_FG_REG_RVBAT);
+
+	vbat = (data * 1000) >> 13;
+	printf("%s: vBAT=%dmV\n", __func__, vbat);
+	return vbat;
+}
+
+u32 s2mpw01_get_avg_vbat(void)
+{
+	u32 vbat_data = 0;
+	u32 vbat_max = 0;
+	u32 vbat_min = 0;
+	u32 vbat_total = 0;
+	u32 i;
+
+	for (i = 0; i < FG_AVG_SAMPLE_CNT; i++) {
+		vbat_data = s2mpw01_get_vbat();
+
+		if (i != 0) {
+			if (vbat_data > vbat_max)
+				vbat_max = vbat_data;
+			else if (vbat_data < vbat_min)
+				vbat_min = vbat_data;
+		} else {
+			vbat_max = vbat_data;
+			vbat_min = vbat_data;
+		}
+		vbat_total += vbat_data;
+	}
+
+	return (vbat_total - vbat_max - vbat_min) / (FG_AVG_SAMPLE_CNT - 2);
+}
+
+u32 s2mpw01_read_soc(void)
+{
+
+	u16 compliment;
+	u32 rsoc;
+
+	compliment = s2mpw01_read_fg_register_word(S2MPW01_FG_REG_RSOC);
+
+	/* 2's compliment format number */
+	if (compliment & (0x1 << 15)) {
+		rsoc = ((~compliment) & 0xFFFF) + 1;
+		rsoc = (rsoc * (-10000)) / (0x1 << 12);
+	} else {
+		rsoc = compliment & 0x7FFF;
+		rsoc = ((rsoc * 10000) / (0x1 << 12));
+	}
+
+	printf("%s: capacity (0x%x:%d)\n", __func__, compliment, rsoc);
+
+	return min(rsoc, 10000) / 10;
+}
+
+u16 s2mpw01_get_ocv(void)
+{
+	u16 data;
+	u32 ocv;
+
+	data = s2mpw01_read_fg_register_word(S2MPW01_FG_REG_ROCV);
+	ocv = (data * 1000) >> 13;
+	printf("%s: OCV %dmV\n", __func__, ocv);
+	return ocv;
+}
+
+void s2mpw01_set_battery_parameter(void)
+{
+	u8 addr, temp;
+	int i, len;
+
+	addr = 0x0c;
+	len = 2;
+
+	for (i = 0; i < len; i++)
+		PMIC_FUELGAUGE_WRITE(i+addr, model_param1[i]);
+
+	temp = 0x34;
+	PMIC_FUELGAUGE_WRITE(0x2C, temp);
+
+	addr = 0x2E;
+	for (i = 0; i < 88; i++)
+		PMIC_FUELGAUGE_WRITE(i+addr, model_param2[i]);
+
+	addr = 0xBC;
+	for (i = 0; i < 22; i++)
+		PMIC_FUELGAUGE_WRITE(i+addr, model_param3[i]);
+}
+
+void s2mpw01_set_model_data(void)
+{
+	u8 temp;
+	unsigned char reg29;
+	unsigned char reg2A;
+	unsigned char reg2B;
+	unsigned char reg29_t;
+	unsigned char reg2A_t;
+	unsigned char reg2B_t;
+	unsigned int dest;
+	unsigned int offset;
+	unsigned int ocv;
+
+	s2mpw01_set_battery_parameter();
+
+	offset = 0x51F; /* Measured vbat - FG vbat */
+
+	/* Backup default offset value */
+	PMIC_FUELGAUGE_READ(0x29, &reg29);
+	PMIC_FUELGAUGE_READ(0x2A, &reg2A);
+	PMIC_FUELGAUGE_READ(0x2B, &reg2B);
+	printf("%s: 1. reg29:0x%02x, reg2A:0x%02x, reg2B:0x%02x, offset:0x%x\n",
+			__func__, reg29, reg2A, reg2B, offset);
+
+	dest = ((reg2B << 16) & 0x0F0000) | ((reg2A << 8) & 0xFF00) | (reg29 & 0xFF);
+	if ((dest > (0x7FFFF - offset)) && (dest < 0x80000))
+	   dest = 0x7FFFF;
+	 else
+	   dest = dest + offset;
+
+	/* Change offset value */
+	PMIC_FUELGAUGE_WRITE(0x2B, ((dest >> 16) & 0x0F) | (reg2B & 0xF0));
+	PMIC_FUELGAUGE_WRITE(0x2A, (dest >> 8) & 0xFF);
+	PMIC_FUELGAUGE_WRITE(0x29, dest & 0xFF);
+
+	PMIC_FUELGAUGE_READ(0x29, &reg29_t);
+	PMIC_FUELGAUGE_READ(0x2A, &reg2A_t);
+	PMIC_FUELGAUGE_READ(0x2B, &reg2B_t);
+	printf("%s: 2. reg29:0x%02x, reg2A:0x%02x, reg2B:0x%02x\n", __func__, reg29_t, reg2A_t, reg2B_t);
+
+	/* Initial soc dump_done */
+	PMIC_FUELGAUGE_READ(S2MPW01_FG_REG_START, &temp);
+	temp |= 0x33;
+	PMIC_FUELGAUGE_WRITE(S2MPW01_FG_REG_START, temp);
+	mdelay(500);
+
+	/* Recover default offset value */
+	PMIC_FUELGAUGE_WRITE(0x29, reg29);
+	PMIC_FUELGAUGE_WRITE(0x2A, reg2A);
+	PMIC_FUELGAUGE_WRITE(0x2B, reg2B);
+
+	PMIC_FUELGAUGE_READ(0x29, &reg29);
+	PMIC_FUELGAUGE_READ(0x2A, &reg2A);
+	PMIC_FUELGAUGE_READ(0x2B, &reg2B);
+	printf("%s: 3. reg29:0x%02x, reg2A:0x%02x, reg2B:0x%02x\n", __func__, reg29, reg2A, reg2B);
+}
+
+void s2mpw01_set_charger_state(int enable)
+{
+	u8 temp;
+
+	/* If enable == 1, set default charging current and enable charging */
+	/* If enable == 0, disable charging */
+	if (enable) {
+		/* Charger enable */
+		PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL1, &temp);
+		temp |= 0x80;
+		PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL1, temp);
+	} else {
+		/* Charger disable */
+		PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL1, &temp);
+		temp &= ~0x80;
+		PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL1, temp);
+	}
+}
+
+void s2mpw01_init_fuelgauge(void)
+{
+	u8 por_state;
+	u32 soc, vbat, ocv;
+
+	PMIC_FUELGAUGE_READ(0x17, &por_state); /* reset condition check */
+	printf("%s: POR state = 0x%02x\n", __func__, por_state);
+
+	soc = s2mpw01_read_soc();
+
+	if ((por_state & 0x10) || (soc < 10)) {
+		s2mpw01_set_charger_state(0);
+		mdelay(100);
+		s2mpw01_set_model_data();
+		s2mpw01_set_charger_state(1);
+		por_state &= ~0x10;
+		PMIC_FUELGAUGE_WRITE(0x17, por_state);
+		printf("%s: Fuelgauge Reset\n", __func__);
+	}
+
+	soc = s2mpw01_read_soc();
+	vbat = s2mpw01_get_avg_vbat();
+	ocv = s2mpw01_get_ocv();
+	printf("%s: vbat(%d) ocv(%d)\n", __func__, vbat, ocv);
+	printf("%s: done\n", __func__);
+}
+
