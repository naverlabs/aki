@@ -114,3 +114,90 @@
 
 	return 0;
 }
+
+static int decon_fill_reverse_one_char(u32 *buf, u32 x, u32 pitch, int num,
+		int low, u32 color, u32 bg_color)
+{
+	int i, j;
+	u32 offset;
+	u32 *ptr;
+
+	if (num < 0 | num > 94)
+		return -1;
+
+	offset = 64 * num;
+
+	for (i = 0; i < FONT_Y; i++) {
+
+		ptr = buf + ((32 - i+low) * pitch) + x;
+
+		for (j = 0; j < (FONT_X/2); j++) {
+			if (font[offset] & (1 << j))
+				ptr[9+j] = color;
+			else
+				ptr[9+j] = bg_color;
+		}
+
+		ptr = ptr + (FONT_X/2);
+		offset++;
+
+		for (j = 0; j < (FONT_X/2); j++) {
+			if (font[offset] & (1 << j))
+				ptr[j-7] = color;
+			else
+				ptr[j-7] = bg_color;
+		}
+
+		offset++;
+	}
+
+	return 0;
+}
+
+
+static int decon_fill_line_char(u32 *buf, u32 x, u8 *str,
+		u32 color, u32 bg_color, int num)
+{
+	int i = 0;
+	int count = 0;
+	struct decon_lcd *lcd_info = decon_get_lcd_info();
+
+	if (num > MAX_WIDTH)
+		count = MAX_WIDTH;
+	else
+		count = num;
+
+	for (i = 0; i < count; i++) {
+		if (decon_fill_reverse_one_char(buf, x + (i * FONT_X),
+					lcd_info->xres, (*str++) - 32, 80, color, bg_color)) {
+			printf("This(%c) character is not supported\n", *(str-1));
+		}
+	}
+
+	num = num - MAX_WIDTH;
+
+	return num;
+}
+
+int cdecon_fill_frame_buffer_ch(u32 *buf, u32 x, u8 *str,
+		u32 color, u32 bg_color)
+{
+	int num = 0;
+	int over_num = 0;
+
+	if (!str)
+		return -1;
+	else
+		num = strlen(str);
+
+	do {
+		num = decon_fill_line_char(buf, x, str, color, bg_color, num);
+		if (num)
+			str = str + MAX_WIDTH;
+	} while(num > 0);
+
+	return 0;
+}
