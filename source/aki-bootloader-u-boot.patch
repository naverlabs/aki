+#include <common.h>
+#include <command.h>
+#include <fastboot.h>
+
+int do_scatter_load_boot(cmd_tbl_t * cmdtp, int flag, int argc, char *argv[])
+{
+	unsigned long long boot_addr, kernel_addr, dtb_addr, ramdisk_addr;
+	struct fastboot_boot_img_hdr *fb_hdr;
+	int kernel_offset;
+	int dtb_offset;
+	int ramdisk_offset;
+	char initrd_size[32];
+    char magic[9];
+	char *p;
+
+	if (argc != 5) goto usage;
+
+	boot_addr = simple_strtoull(argv[1], NULL, 16);
+	kernel_addr = simple_strtoull(argv[2], NULL, 16);
+	ramdisk_addr = simple_strtoull(argv[3], NULL, 16);
+	dtb_addr = simple_strtoull(argv[4], NULL, 16);
+
+	fb_hdr = (struct fastboot_boot_img_hdr *)boot_addr;
+
+	printf("page size: 0x%08x\n", fb_hdr->page_size);
+	printf("kernel size: 0x%08x\n", fb_hdr->kernel_size);
+	printf("ramdisk size: 0x%08x\n", fb_hdr->ramdisk_size);
+	printf("DTB size: 0x%08x\n", fb_hdr->second_size);
+
+#ifndef CONFIG_AUTO_DOWNLOAD_EMMC
+	if (fb_hdr->page_size == 0 || fb_hdr->kernel_size == 0
+		|| fb_hdr->ramdisk_size == 0 || fb_hdr->second_size == 0)
+	{
+		run_command(CONFIG_COMMAND_GPT_ENV, 0);
+
+		p = getenv("akipartver");
+		if (!p || strcmp(p, AKI_PART_VER)) {
+			/* NAVERLABS:
+			* EFS will be formatted when partition changed.
+			* We store partition version to prevent format EFS again for same
+			* partition table version.
+			*/
+			printf("partition changed. set akipartver: %s\n", AKI_PART_VER);
+			//printf("partition updated(%s). format EFS...\n", AKI_PART_VER);
+			setenv("akipartver", AKI_PART_VER);
+			saveenv();
+			//run_command(CONFIG_COMMAND_FORMAT_EFS, 0);
+		}
+
+		run_command(CONFIG_COMMAND_FASTBOOT, 0);
+	}
+#endif
+
+    memcpy(magic, fb_hdr, 8);
+    magic[8] = 0;
+    printf("magic name:%s\n", magic);
+    
+	kernel_offset = fb_hdr->page_size;
+	ramdisk_offset = kernel_offset + ((fb_hdr->kernel_size + fb_hdr->page_size - 1) / fb_hdr->page_size) * fb_hdr->page_size;
+	dtb_offset = ramdisk_offset + ((fb_hdr->ramdisk_size + fb_hdr->page_size - 1) / fb_hdr->page_size) * fb_hdr->page_size;
+
+	memcpy((void *)kernel_addr, (const void *)(boot_addr + kernel_offset), (size_t)fb_hdr->kernel_size);
+	memcpy((void *)ramdisk_addr, (const void *)(boot_addr + ramdisk_offset), (size_t)fb_hdr->ramdisk_size);
+	memcpy((void *)dtb_addr, (const void *)(boot_addr + dtb_offset), (size_t)fb_hdr->second_size);
+
+	sprintf(initrd_size, "0x%x", fb_hdr->ramdisk_size);
+	setenv("rootfslen", initrd_size);
+
+    printf("kernel_offset, ramdisk_offset, dtb_offset\n");
+
+
+	return 0;
+
+usage:
+	printf("Usage:\n%s\n", cmdtp->usage);
+	return -1;
+}
+
+U_BOOT_CMD(
+	scatter_load_boot,	5,	0,	do_scatter_load_boot,
+	"scatter_load_boot - scatter load kernel, dtb, ramdisk from boot/recovery.img",
+	"scatter_load_boot {boot/recovery addr} {kernel addr} {dtb addr} {ramdisk addr}\n"
+);
