diff -uNra a/board/samsung/espresso7570/charger_s2mpw01.c b/board/samsung/espresso7570/charger_s2mpw01.c
--- a/board/samsung/espresso7570/charger_s2mpw01.c	1970-01-01 09:00:00.000000000 +0900
+++ b/board/samsung/espresso7570/charger_s2mpw01.c	2018-04-01 16:00:04.658650515 +0900
@@ -0,0 +1,273 @@
+/*
+ * Charger driver for S2MPW01
+ * Copyright@ Samsung Electronics Co. LTD
+ *
+ * This software is proprietary of Samsung Electronics.
+ * No part of this software, either material or conceptual may be copied or
+ * distributed, transmitted, transcribed, stored in a retrieval system or
+ * translated into any human or computer language in any form by any means,
+ * electronic, mechanical, manual or otherwise, or disclosed to third parties
+ * without the express written permission of Samsung Electronics.
+
+ * Alternatively, this program is free software in case of open source project;
+ * you can redistribute it and/or modify
+ * it under the terms of the GNU General Public License version 2 as
+ * published by the Free Software Foundation.
+ */
+#include <asm/types.h>
+#include <asm/arch/pmic_s2mpw01.h>
+#include "../../../drivers/speedy/speedy.h"
+
+typedef enum {false, true} bool;
+
+#define CHARGE_SOURCE_USB       0
+#define CHARGE_SOURCE_AC        1
+#define ENABLE_EXAMPLE_CHARGER_CONFIGURATION  1
+
+#define REGULATION_VOLTAGE_SAMPLE 4350
+#define TOPOFF_CURRENT_SAMPLE     50
+#define CHARGING_CURRENT_SAMPLE   400
+
+int s2mpw01_detect_battery(void)
+{
+	u8 data;
+	/* Check battery detect */
+	PMIC_CHARGER_READ(S2MPW01_CHG_REG_STATUS2, &data);
+	return (data & DET_BAT_STATUS_MASK) ? true : false;
+}
+
+int s2mpw01_get_vbus_state(void)
+{
+	u8 data;
+	/* Get vbus state. if vbus is valid, return 1 */
+	PMIC_CHARGER_READ(S2MPW01_CHG_REG_STATUS1, &data);
+	return (data & CHGVIN_STATUS_MASK) ? true : false;
+}
+
+void s2mpw01_set_regulation_voltage(int float_voltage)
+{
+	int data;
+	u8 temp;
+
+	if (float_voltage <= 4200)
+		data = 0;
+	else if (float_voltage > 4200 && float_voltage <= 4550)
+		data = (float_voltage - 4200) / 50;
+	else
+		data = 0x7;
+
+	printf("%s: float voltage: %d\n", __func__, float_voltage);
+
+	PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL5, &temp);
+	temp &= ~0x0E;
+	temp |= (data << 1);
+	PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL5, temp);
+}
+
+void s2mpw01_set_charging_current(int charging_current)
+{
+	int data;
+	int current;
+	u8 temp;
+
+	if (charging_current > 200)
+		current = 200;
+
+	if (current <= 150)
+		data = 0;
+	else if (current > 150 && current <= 400)
+		data = (current - 150) / 50;
+	else
+		data = 0x5;
+
+	printf("%s: charging_current: %d\n", __func__, charging_current);
+
+	PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL2, &temp);
+	temp &= ~0x0E;
+	temp |= (data << 1);
+	PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL2, temp);
+
+	/* Additional Charging path on when Very low battery condtion */
+	if (charging_current > 200) {
+		/* Additional Current 7/8 * Main */
+		temp = 0x7f;
+		PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL7, temp);
+
+		/* Additional Current Path On */
+		PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL5, &temp);
+		temp |= (1 << 5);
+		PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL5, temp);
+	} else {
+		/* Additional Current Path Off */
+		PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL5, &temp);
+		temp &= ~(1 << 5);
+		PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL5, temp);
+
+		/* Additional Current 8/8 * Main */
+		temp = 0xff;
+		PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL7, temp);
+	}
+}
+
+void s2mpw01_set_topoff_current(int current_limit)
+{
+	int data;
+	u8 temp;
+
+	if (current_limit <= 5)
+		data = 0;
+	else if (current_limit > 5 && current_limit <= 10)
+		data = (current_limit - 5) / 5;
+	else if (current_limit > 10 && current_limit < 18)
+		data = (current_limit - 10) / 5 * 2 + 1;
+	else if (current_limit >= 18 && current_limit < 20)
+		data = 5;	  /* 17.5 mA */
+	else if (current_limit >= 20 && current_limit < 25)
+		data = 4;
+	else if (current_limit >= 25 && current_limit <= 40)
+		data = (current_limit - 25) / 5 + 6;
+	else if (current_limit > 40 && current_limit <= 100)
+		data = (current_limit - 40) / 10 + 9;
+	else
+		data = 0x0F;
+
+	printf("%s: top-off current: %d\n", __func__, current_limit);
+
+	PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL4, &temp);
+	temp &= ~0xF0;
+	temp |= (data << 4);
+	PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL4, temp);
+}
+
+/* Load OTP value.
+ * After this function, FG(0x26~0x2D) and CHG(0x17~0x34) values are reset.
+ * This function must be the first operation after power on
+ * for S2MPW01 charger/fuelgauge.
+ */
+void s2mpw01_load_otp(void)
+{
+	u8 temp;
+
+	PMIC_COMMON_READ(0x10, &temp);
+	temp |= 0x20;
+	PMIC_COMMON_WRITE(0x10, temp);
+
+	PMIC_COMMON_READ(0x0E, &temp);
+	temp |= 0x80;
+	PMIC_COMMON_WRITE(0x0E, temp);
+
+	mdelay(100);
+
+	PMIC_COMMON_READ(0x10, &temp);
+	temp &= ~0x20;
+	PMIC_COMMON_WRITE(0x10, temp);
+
+	PMIC_COMMON_READ(0x0E, &temp);
+	temp &= ~0x80;
+	PMIC_COMMON_WRITE(0x0E, temp);
+
+	printf("%s: done\n", __func__);
+}
+
+u8 s2mpw01_get_chip_version(void)
+{
+	u8 pmicid, pmicver;
+
+	PMIC_COMMON_READ(S2MPW01_COMMON_PMIC_ID, &pmicid);
+	pmicver = (pmicid & S2MPW01_COMMON_PMIC_VERSION_MASK);
+	printf("%s: PMIC chip version: 0x%X\n", __func__, pmicver);
+	return pmicver;
+}
+
+void s2mpw01_set_charger_current(int type)
+{
+	/* Example current setting for charger */
+	s2mpw01_set_regulation_voltage(REGULATION_VOLTAGE_SAMPLE);
+
+	/* Set 50mA for topoff current */
+	s2mpw01_set_topoff_current(TOPOFF_CURRENT_SAMPLE);
+
+	switch (type) {
+	case CHARGE_SOURCE_AC:
+	case CHARGE_SOURCE_USB:
+	default:
+		/* Set 400mA for charging current */
+		s2mpw01_set_charging_current(CHARGING_CURRENT_SAMPLE);
+		break;
+	}
+}
+
+void s2mpw01_set_charger_state(int buck, int enable)
+{
+	u8 temp;
+
+	/* If enable == 1, set default charging current and enable charging */
+	/* If enable == 0, disable charging */
+	if (enable) {
+		/* set 400mA for charging current */
+		s2mpw01_set_charging_current(CHARGING_CURRENT_SAMPLE);
+
+		/* Charger enable */
+		PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL1, &temp);
+		temp |= 0x80;
+		PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL1, temp);
+	} else {
+		/* Charger disable */
+		PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL1, &temp);
+		temp &= ~0x80;
+		PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL1, temp);
+	}
+}
+
+#if ENABLE_EXAMPLE_CHARGER_CONFIGURATION
+void s2mpw01_configure_charger_state(void)
+{
+	if (s2mpw01_detect_battery()) {
+		s2mpw01_set_charger_current(0);
+		s2mpw01_set_charger_state(1, 1);
+		mdelay(100);
+	} else {
+		printf("%s: No battery, charger off\n", __func__);
+		s2mpw01_set_charger_state(1, 0);
+	}
+}
+#endif
+
+void s2mpw01_init_charger(void)
+{
+	u8 temp;
+	u8 chip_revision;
+
+	/* Load OTP value */
+	s2mpw01_load_otp();
+
+	PMIC_CHARGER_WRITE(0x25, 0x82);
+
+	/* Check the chip revision */
+	chip_revision = s2mpw01_get_chip_version();
+	if (chip_revision < EVT_3)
+		printf("%s: Warning, old version:%d\n", __func__, chip_revision);
+
+	/* Increase debounce time & CHGIN to VBATT minimum turn on threshold level decrease */
+	PMIC_CHARGER_WRITE(0x2B, 0x72);
+
+	/* IVR Level decrease */
+	PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL4, &temp);
+	temp &= 0xF1;
+	temp |= 0x00;
+	PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL4, temp);
+
+	/* MRSTB enable & WTSR enable */
+	PMIC_CHARGER_READ(S2MPW01_CHG_REG_CTRL9, &temp);
+	temp &= 0x77;
+	temp |= 0x88;
+	PMIC_CHARGER_WRITE(S2MPW01_CHG_REG_CTRL9, temp);
+
+	/* RID operation setting */
+	PMIC_CHARGER_WRITE(0x20, 0x8A);
+
+#if ENABLE_EXAMPLE_CHARGER_CONFIGURATION
+	s2mpw01_configure_charger_state();
+#endif
+}
+
